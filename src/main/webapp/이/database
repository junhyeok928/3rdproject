CREATE TABLE cart (
	NO NUMBER PRIMARY KEY,
	title varchar2(30),
	name varchar2(20),
	compose number(5,0),
	price NUMBER(10,0),
	disprice number(10,0),
	finprice number(10,0)
);
INSERT INTO cart values(1,'정보처리기사 필기', '전준혁', 95, 99000, 33900, 66000);
INSERT INTO cart values(2,'정보처리기사 실기', '유진선', 57, 120000, 21000, 99000);

SELECT * FROM cart;

DELETE FROM cart WHERE NO=2;

DROP TABLE cart;

